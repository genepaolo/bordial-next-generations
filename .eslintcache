nts/Counties\";\n\nclass Services extends Component {\n    constructor(props){\n        super(props);\n        this.state = {\n            active: 'services'\n        }\n    }\n\n    displayPageBody(){\n        return(\n            <div className=\"page-body\" id=\"services-body\">\n                <div>\n                    <h4>OUR SERVICES</h4>\n                    <ul>\n                        <li>Primary Care</li>\n                        <li>Chronic Disease Management</li>\n                        <li>Home Health Coordination</li>\n                        <li>Preventive, Follow-up and Wellness Care</li>\n                        <li>Physical Exam and Annual Wellness Visit</li>\n                        <li>Post Hospital Discharge Follow-ups</li>\n                        <li>In-home Diagnostic and Laboratory testing</li>\n                        <li>Medication Evaluation and Management</li>\n                        <li>Medical supply and equipment orders</li>\n                        <li>Referrals to Specialists- Wound care, Psychiatrist, Cardiologist, Pain Management, Dermatologist, Ophthalmologist, Podiatrist etc.</li>\n                        <li>Coordination of Social Services</li>\n                    </ul>\n                    <p>We accept Medicare B</p>\n                </div>\n                <br/>\n                <div>\n                    <h4>Why Choose Cordial Healthcare Providers</h4>\n                    <ul>\n                        <li>Our team consists of highly trained and qualified Board-certified Doctors, Nurse Practitioners, and Physician Assistants.</li>\n                        <li>Our Medical Providers are available 7 days a week.</li>\n                        <li>Fast and easy appointment scheduling.</li>\n                        <li>Appointments are available within 24 to 48 hours. We even offer same-day appointments if the schedule permits. </li>\n                        <li>Home Health Referrals are submitted within 24 to 48 business hours.</li>\n                        <li>We take the time to get to know our patients and address all of their medical needs.</li>\n                        <li>We strive to provide the best quality healthcare wherever you may be.</li>\n                    </ul>\n                </div>\n                <br/>\n                <div>\n                <h4>We Care for Patients in the following home settings</h4>\n                    <ul>\n                        <li>Private Homes</li>\n                        <li>Assisted Living Facilities</li>\n                        <li>Group Homes</li>\n                        <li>Adult Foster Care Homes (AFC)</li>\n                    </ul>\n                    \n                </div>\n            </div>\n        );\n    }\n\n\n    render(){\n        return(\n            <div className=\"body\" id=\"services-body\">\n                 <div className=\"page-title\">\n                    <p className=\"page-title-p\">SERVICES</p>\n                </div>\n                {this.displayPageBody()}\n                <Counties/>\n            </div>\n        );\n    }\n}\n\nexport default Services;","/Users/genepaolo/Documents/Work/CordialMD/cordialmd/src/pages/ContactUs.jsx",["97"],"import React, { Component, useState } from \"react\";\nimport emailjs from 'emailjs-com';\nimport \"../styles/styles.scss\";\nimport \"../styles/styles600px.scss\";\nimport 'bootstrap/dist/css/bootstrap.min.css';\nimport'bootstrap/dist/js/bootstrap.bundle.min';\n\nclass ContactUs extends Component {\n    constructor(props){\n        super(props);\n        this.state = {\n            active: 'contact-us',\n            name:'',\n            email:'',\n            subject:'',\n            msg:''\n        }\n        this.name='';\n        this.email='';\n        this.subject='';\n        this.msg='';\n        this.sendEmail = this.sendEmail.bind(this);\n    }\n\n    sendEmail(e) {\n        e.preventDefault();    //This is important, i'm not sure why, but the email won't send without it\n    \n        emailjs.sendForm('gmail', 'template_i7u578m', e.target, 'user_fysxm9swVB9DFCDMGbTB3')\n          .then((result) => {\n              window.location.reload()  //This is if you still want the page to reload (since e.preventDefault() cancelled that behavior) \n          }, (error) => {\n              console.log(error.text);\n          });\n    }\n\n    displayPageBody(){\n        return(\n            <div className=\"page-body\" id=\"contactus-page-body\">\n                <br></br>\n                <p>Cordial Healthcare Providers</p>\n                <p>Email: <a href =\"mailto: cordialmmg@gmail.com\">cordialmmg@gmail.com</a></p>\n                <br/>\n                <p>127 S. Brand Blvd</p>\n                <p>Suite 333</p>\n                <p>Glendale, CA 91204</p>\n                <br></br>\n                <p>Thank you for choosing our services. For service inquiries and other concerns, you may visit our office, leave us a message using the form below, or send us an email directly. One of our team members will be in contact as soon as possible.</p>\n                <br></br>\n                <form onSubmit={this.sendEmail}>\n                    <div class=\"form-row\">\n                        <div class=\"col-12\">\n                        <label >Name <span class=\"required\">*</span></label>\n                        <input type=\"text\" ref={(c) => this.name = c} class=\"form-control\" name=\"from_name\"id=\"cu-name\" required/>\n                        {/* <input type=\"text\" value={this.state.name} onChange={this.handleChangeName} class=\"form-control\" id=\"cu-name\" required/> */}\n                        </div>\n                    </div>\n                    <div class=\"form-row\">\n                        <div class=\"col-12\">\n                        <label >Email <span class=\"required\">*</span></label>\n                        <input type=\"text\"ref={(c) => this.email = c} class=\"form-control\" name=\"from_email\"id=\"cu-email\" required/>\n                        {/* <input type=\"text\" value={this.state.email} onChange={this.handleChangeEmail} class=\"form-control\" id=\"cu-email\" required/> */}\n                        </div>\n                    </div>\n                    <div class=\"form-row\">\n                        <div class=\"col-12\">\n                        <label >Subject <span class=\"required\">*</span></label>\n                        <input type=\"text\" ref={(c) => this.subject = c} class=\"form-control\" name=\"subject\"id=\"cu-subject\" required/>\n                        {/* <input type=\"text\" value={this.state.subject} onChange={this.handleChangeSubject} class=\"form-control\" id=\"cu-subject\" required/> */}\n                        </div>\n                    </div>\n                    <div class=\"form-row\">\n                        <div class=\"col-12\">\n                        <label >Message <span class=\"required\">*</span></label>\n                        <textarea type=\"text\"  ref={(c) => this.msg = c} class=\"form-control\" name=\"message\"id=\"cu-msg\" required/>\n                        {/* <textarea type=\"text\" value={this.state.msg} onChange={this.handleChangeMsg} class=\"form-control\" id=\"cu-msg\" required/> */}\n                        </div>\n                    </div>\n                    <br/>\n                        <button id=\"cu-button\"class=\"btn\" type=\"submit\">Submit Message</button>\n                    </form>\n\n            </div>\n        );\n    }\n\n    render(){\n        return(\n            <div className=\"body\" id=\"contact-us-body\">\n                <div className=\"page-title\">\n                    <p className=\"page-title-p\">CONTACT US</p>\n                </div>\n                {this.displayPageBody()}\n            </div>\n        );\n    }\n}\n\nexport default ContactUs;","/Users/genepaolo/Documents/Work/CordialMD/cordialmd/src/pages/Career.jsx",["98","99"],"import React, { Componen