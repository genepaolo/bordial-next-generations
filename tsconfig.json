{
    "compilerOptions": {
      "baseUrl": ".",
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "paths": {
        "@/*": ["./src/*"]
      }
    },
    "include": ["src/components", "src/custom.d.ts"],

    "exclude": ["node_modules"]
  }
  